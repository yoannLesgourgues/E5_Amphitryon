<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:id="@+id/textView_idCommande"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:textSize="15dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="idCommande "
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/textView_numTable"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:layout_marginLeft="10dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="numTable "
        app:layout_constraintStart_toEndOf="@+id/textView_idCommande"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView_idCommande"/>

    <TextView
        android:id="@+id/textView_dateService"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:layout_marginLeft="10dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="dateService "
        app:layout_constraintStart_toEndOf="@+id/textView_numTable"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView_numTable"/>

    <TextView
        android:id="@+id/textView_heureCommande"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:layout_marginLeft="10dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="heureCommande "
        app:layout_constraintStart_toEndOf="@+id/textView_dateService"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView_dateService"/>

    <TextView
        android:id="@+id/textView_etatCommande"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        android:layout_marginLeft="10dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="etatCommande "
        app:layout_constraintStart_toEndOf="@+id/textView_heureCommande"
        app:layout_constraintBaseline_toBaselineOf="@+id/textView_heureCommande"/>
</androidx.constraintlayout.widget.ConstraintLayout>